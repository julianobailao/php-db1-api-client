{
    "name": "julianobailao/php-db1-api-client",
    "type": "library",
    "description": "Domus Erp - Api Client.",
    "keywords": [
        "juliano bailao",
        "db1",
        "domus erp",
        "erp",
        "api client"
    ],
    "homepage": "https://github.com/julianobailao/php-db1-api-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Juliano Bail√£o",
            "email": "julianobailao@gmail.com",
            "homepage": "http://github.com/julianobailao",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5|~5.6|~7.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "JulianoBailao\\DomusApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulianoBailao\\DomusApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
